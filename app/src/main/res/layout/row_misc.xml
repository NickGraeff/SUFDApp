<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <import type="android.view.View"/>
        <variable
            name="misc"
            type="java.util.List"
            />
    </data>
        <LinearLayout
            android:id="@+id/move_misc"
            android:layout_width="fill_parent"
